{
  "name": "siko",
  "version": "0.4.2",
  "description": "Runtime execution analyzer for JavaScript & TypeScript - identify dead code with confidence",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "siko": "dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "npm run build && npm test",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\""
  },
  "keywords": [
    "dead-code",
    "code-analysis",
    "runtime-analysis",
    "unused-code",
    "instrumentation",
    "babel",
    "typescript",
    "javascript",
    "coverage",
    "testing",
    "refactoring",
    "static-analysis",
    "code-quality",
    "ci-cd",
    "developer-tools"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neu-msinha/siko.git"
  },
  "bugs": {
    "url": "https://github.com/neu-msinha/siko/issues"
  },
  "homepage": "https://siko.dev",
  "author": "Mayukh Sinha <mayukh2026@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/babel__core": "^7.20.5",
    "@types/babel__traverse": "^7.20.5",
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "@types/source-map": "^0.5.2",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "eslint": "^9.39.2",
    "globals": "^15.15.0",
    "jest": "^29.7.0",
    "minimatch": "^10.1.2",
    "prettier": "^3.8.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.54.0"
  },
  "dependencies": {
    "@babel/core": "^7.23.0",
    "@babel/parser": "^7.23.0",
    "@babel/traverse": "^7.23.0",
    "@babel/types": "^7.23.0",
    "@types/minimatch": "^5.1.2",
    "chalk": "^4.1.2",
    "commander": "^11.0.0",
    "minimatch": "^10.0.0",
    "source-map": "^0.7.6"
  }
}
